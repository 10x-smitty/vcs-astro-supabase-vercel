---
import Layout from "../layouts/Layout.astro";
---

<Layout>
    <div class="flex overflow-hidden flex-col bg-white" role="main">
    <section class="flex overflow-hidden flex-col justify-center items-center py-24 w-full text-center bg-white max-md:max-w-full" aria-labelledby="about-heading">
